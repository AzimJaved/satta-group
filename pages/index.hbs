        <link rel='stylesheet' href="css/style.css">



    <script>
        function onLoad(){
            var html = "";
            var players = [ {{ data }} ]
        players.forEach( (player) =>{
            var txt = '<input type="checkbox" name="players" value="' +player+'">'+player+'</input><br>'
            html += txt;
        })
        document.getElementById('fmcheckbox').innerHTML += html;
    }
    window.onload = onLoad;
    </script>

        <h1 align="center" class = "Heading"> Satta application me aapka swaagat hai </h1>
            <br>

    <form method="POST" action="/teams" id="tmform">
        <div id="fmcheckbox">
        </div>
        <input type="text" style="display:none" placeholder="PlayerName" name="playerName" />
        <br>
        <br>
        <input type="submit" value="Submit" />
    </form>
</section>